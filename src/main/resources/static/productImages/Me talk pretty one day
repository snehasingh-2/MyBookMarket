    // Some SVG icons modified CC-BY 4.0 https://creativecommons.org/licenses/by/4.0/ https://github.com/google/material-design-icons
    (function () {
        function cloneToDoc(n) {
            var clone = document.createElementNS(n.namespaceURI, n.nodeName);
            for (var i = 0, len = n.attributes.length; i < len; ++i) { var a = n.attributes[i]; if (/^xmlns\b/.test(a.nodeName)) continue; clone.setAttributeNS(a.namespaceURI, a.nodeName, a.nodeValue) }
            for (var i = 0, len = n.childNodes.length; i < len; ++i) { var c = n.childNodes[i]; clone.insertBefore(c.nodeType == 1 ? cloneToDoc(c, document) : document.createTextNode(c.nodeValue), null) }
            return clone
        }
        var svgPath = "/svg/versioned/v-638192224008196612/icons.svg";
        if (localStorage && typeof localStorage['svg'] !== 'undefined' && localStorage['svgPath'] === svgPath) {
            var svg = new DOMParser().parseFromString(localStorage['svg'], 'application/xml');
            document.body.insertBefore(window.isIE9 ? cloneToDoc(svg.documentElement) : svg.documentElement, document.body.childNodes[0]);
        } else {
            var ajax = new XMLHttpRequest(); ajax.open("GET", svgPath, true); ajax.responseType = "document"; ajax.send();
            ajax.onload = function (e) {
                var svg = ajax.responseXML;
                try { if (localStorage) { localStorage['svg'] = new XMLSerializer().serializeToString(svg); localStorage['svgPath'] = svgPath; } } catch (e) { } 
                document.body.insertBefore(window.isIE9 ? cloneToDoc(svg.documentElement) : svg.documentElement, document.body.childNodes[0]);
            };
        }
    })();
